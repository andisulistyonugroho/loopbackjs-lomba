{
  "name": "competition",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "eventId": {
      "type": "number",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "dataType": "TEXT"
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "logo": {
      "type": "string"
    },
    "userId": {
      "type": "number",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "event": {
      "type": "belongsTo",
      "model": "event",
      "foreignKey": "eventId",
      "options": {
        "nestRemoting": true
      }
    },
    "contestantTypes": {
      "type": "hasMany",
      "model": "contestantType",
      "foreignKey": "competitionId",
      "options": {
        "nestRemoting": true
      },
      "through": "mapCompetitionContestantType"
    },
    "contestant": {
      "type": "hasMany",
      "model": "contestant",
      "foreignKey": "competitionId"
    },
    "jurors": {
      "type": "hasMany",
      "model": "Users",
      "foreignKey": "competitionId",
      "keyThrough": "userId",
      "options": {
        "nestRemoting": true
      },
      "through": "juryPanel"
    },
    "competitionScoreCriteria": {
      "type": "hasMany",
      "model": "competitionScoreCriteria",
      "foreignKey": "competitionId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superadmin",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "jury",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "member",
      "permission": "ALLOW"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "jury",
      "permission": "ALLOW",
      "property": "unScoredUser"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "jury",
      "permission": "ALLOW",
      "property": "unScoredUserCount"
    }
  ],
  "methods": {}
}
